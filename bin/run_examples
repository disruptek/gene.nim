#!/usr/bin/env bash

for file in examples/*; do
  echo '$' $file

  if [ "$file" = "examples/pipe.gene" ]; then
    echo TEST | $file
  elif [ "$file" = "examples/repl-on-demand.gene" ] || [ "$file" = "examples/repl-on-error.gene" ]; then
    echo "SKIPPED!"
  elif [ -f "$file" ] && [ -x "$file" ]; then
    "$file"
  else
    echo "NOT EXECUTABLE!"
  fi

  echo
  echo
done
