#!/usr/bin/env gene

(fn ^^async f a
  (var future
    (gene/sleep_async 2500)
  )
  (future .on_success (->
    (println a)
  ))
  future
)
(await (f "first"))
(f "second")
(println 0)
(gene/sleep 1000)
(println 1)
(gene/sleep 1000)
(println 2)
(gene/sleep 1000)
(println 3)

#< Output should look like
first
0
1
2
second
3
#>#
